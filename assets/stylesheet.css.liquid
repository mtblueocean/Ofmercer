/* #Base Styles
================================================== */

html { font-size: 62.5%; height: 100%;}

body {
    background: {{ settings.background }};
    font-family:"proxima-nova",Arial,Helvetica, sans-serif;
	font-size: {{ settings.font-size }};
    text-shadow: 1px 1px 1px rgba(0,0,0,0.004);
    font-style: normal;
    font-weight: 400;
    line-height: 1;
    position: relative; 
    color: {{ settings.text_color }};
    text-rendering: optimizeLegibile;
    width: 100%;
    height: 100%;
    margin: 0px;
    padding: 0px;
    letter-spacing: .05em
 }

/* #Typography
================================================== */

    body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, p, blockquote, th, td { margin: 0; padding: 0; direction: ltr; }

	h1, h2, h3, h4, h5, h6, label {
		color: black;
		/*font-family:{{ settings.headings_font }};*/
  font-family: "proxima-nova",Arial,Helvetica, sans-serif;letter-spacing: .12em;
		font-weight: 300;
    text-transform: {{ settings.headings-text-transform }};
        }
	h1 a, h2 a, h3 a, h4 a, h5 a, h6 a { font-weight: inherit;  }
	h1 { font-size: 13px; margin-bottom: 10px; letter-spacing: .15em;font-family:"AvenirNextLTPro-Demi",Arial,Helvetica,sans-serif;}
	h2 { font-size: 12px; margin-top:12px;margin-bottom: 12px;letter-spacing: .15em;font-family:"AvenirNextLTPro-Demi",Arial,Helvetica,sans-serif;}
	h3 { font-size: 15px; margin-bottom: 8px; letter-spacing:.15em; padding-top:5px;}
	h4 { font-size: 13px; margin-bottom: 5px; }
	h5 { font-size: {{ settings.h5-size }}; margin-bottom: 5px; }
	h6 { font-size: {{ settings.h6-size }}; margin-bottom: 5px; }
    hr { border: solid #ddd; border-width: 1px 0 0; clear: both; margin: 22px 0 21px; height: 0; }
    
    .subheader { line-height: 1.3; color: #6f6f6f; font-weight: 300; margin-bottom: 17px; }

	p { margin: 0 0 17px 0; line-height: 1.7; font-size: 13px;font-weight:;font-family:"proxima-nova",Arial,Helvetica, sans-serif; letter-spacing:.0em }
	p img { margin: 0; }
    p.lead { font-size: 17.5px; line-height: 1.6; margin-bottom: 17px; }

    em, i { font-style: italic; line-height: inherit; }
    strong, b { font-weight: bold; line-height: inherit; }
    small { font-size: 90%; line-height: inherit; }
    code { font-weight: bold; background: #ffff99; }
    
    b {font-weight: bold;}
    i { font-style: italic; }


