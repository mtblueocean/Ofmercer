	width:54%;
	float:left; 
}

@media (max-width: 700px) {  
  .one-half-column-left {
    width: 100%;
  }
}

.one-half-column-right {
	width:43%;
	float:left;
	display: inline;
	margin-left: 30px;
}

@media (max-width: 350px) {  
  .one-half-column-right {
    width: 85%;
    padding-left: 0;
    padding-right: 0;
  }
}

.one-half-column-left-second {
	width:38%;
	float:left;
	margin-top: 15px;  
}

@media (max-width: 700px) {  
  .one-half-column-left-second {
    width: 100%;
  }
}

.one-half-column-right-second {
	width:54%;
	float:left;
	display: inline;
	margin-top: 15px;
	margin-right: 50px
}



