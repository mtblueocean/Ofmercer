{% assign color_field = settings.color_swatches %}
{% assign color_list = color_field | newline_to_br | replace: '<br />', '|' | split: '|' %}
{% for colors in color_list %}
  {% assign color = colors | split: ":" %}
  
.color-{{ color[0] | remove: " " | strip }} {
	label {
		background-color:#{{ color[1] | remove: " " | strip }};	
	}
}

form .colors {
  .color-{{ color[0] | remove: " " | strip }} { 
    > div {
      background-color:#{{ color[1] | remove: " " | strip }};
    }
  }
}   

{% endfor %}

.swatch {
	@include clearfix();
	.sw-col {
		float: left;
		position: relative;
	}
	.swatch-title {
		margin: 0;
		@extend .unvert-y;
	}
	.swatch-container {
		@include clearfix();
		@extend .unvert-y;
	}
	.swatch-element {
		float: left;
		margin-right: 7%;
		margin-bottom: 7%;
		&:last-child {
			margin-right: 0;
		}
		&.active {
			label {
				&:after {
					border: 1px solid #000;	
				}
			}
		}
		&.color-white {
			label {
				border: 1px solid #f1f1f1;
			}
		}
		&.soldout {
			label {
				@include prefix('opacity', .2);
			}
		}		
		label {
			display: block;
			width: 20px;
			height: 20px;
			cursor: pointer;
			@include prefix('border-radius', '50%');
			background-size: 32px 32px;
			background-position: center center;
			border-width: 0;
			border-style: solid;
			border-color: #9b9b9b;
			position: relative;
			&:after {
				content: '';
				position: absolute;
				width: 24px;
				height: 24px;
				background: transparent;
				border: 1px solid #c2c2c2;
				top: 50%;
				left: 50%;
				margin-top: -13px;
				margin-left: -13px;				
				@include prefix('border-radius', '50%');
			}	
		}				
		input {
			display: none;
			&:checked {
				+ label {
					border-width: 2px;
				}
			}			
		}
	}
	@include breakpoint(ml){
		.swatch-container {
			width: 100%;
		}
		.swatch-element {
			margin-right: 5%;
			margin-bottom: 5%;
			label {
				width: 26px;
				height: 26px;
				&:after {
					width: 30px;
					height: 30px;
					margin-top: -16px;
					margin-left: -16px;
				}
			}
		}
	}
}

.swatch .swatch-element.gift-card {
  margin-bottom: 0;
  label {
    border: 1px solid #c2c2c2;
    width: auto;
    height: auto;
    font-size: 14px;
    padding: 5px 10px;
    border-radius: 0;
    &:after {
