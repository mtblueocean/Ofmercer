.selector-wrapper {
	text-align: center;
    span.size-msg {
      display: none;
      color: red;
      margin-bottom: 10px;
    }
  	&.show #size-trigger {
      position: absolute;
    }
	#size-trigger {
	text-transform: none;
	cursor: pointer;
	z-index: 3;
    display: block;
    font-size: 1.5rem;
    letter-spacing:.03em;
    left: 0;
    width: 100%;
    text-align: center;  
	-webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;      
		font-weight: lighter;
		font-family: 'neuzeit-grotesk';  
		.select-text {
			display: inline-block;
		}  
		&:after {
			content: "";
			display: block;
			width: 9px;
			height: 9px;
			margin-left: 5px;
			display: inline-block;
			background: url("{{ 'arrow-icon-up.svg' | asset_url | split:'?' | first }}") center center no-repeat;
		}
	}		
	#size-guide-trigger {
	
	}	
	&.show {
		.dummy-select {
			display: block;
		}
		#size-trigger {
			color: #ccc;	
			&:after {
				background: url("{{ 'arrow-icon-down.svg' | asset_url | split:'?' | first }}") center center no-repeat;			
			}
		}
	}
	@media screen and (max-width: 919px){
		text-align: right;
		#size-guide-trigger {
	    bottom: auto;
	    margin-left: 10px;
	    position: relative;	
	  margin-top: 5px;	    
    }	
    #size-trigger {
	    position: relative;
	    width: auto;
	    bottom: auto;
	    display: inline-block;	  
			.select-text {
				display: none;
			}  	    
    }
    .pf-vert-y {
	    -webkit-transform: none !important;
	    -moz-transform: none !important;
	    -ms-transform: none !important;
	    -o-transform: none !important;
	    transform: none !important;
	    top: 13px !important;
    }	    
	}	
}

.dummy-select {
	margin: 0;
	text-align: center;
	text-transform: uppercase;	
	display: none;
	
	li {
		display: inline-block;
		cursor: pointer;	
		width: auto;
		min-width: 2.2rem;
		padding: 0 .5rem;
		height: 2.2rem;
		line-height: 2.2rem;
		margin: 0;
		border: 1px solid transparent;
		text-align: center;
		font-size: 1rem;
		&.active {
			border: 1px solid #000;
		}
		&.unavailable {
			@include prefix('opacity', .5);
		}
	}
  border: 1px solid #ccc;
  position: absolute;
  bottom: -25px;
  background: #fff;
  z-index: 2;
  display: none;
  width: 100%;
  padding: 15px 0px 45px;

	float: inherit;
	li {
		height: 2.5rem;	
		font-size: 1.3rem;
		
		line-height: 1.3;
		padding: 5px 12px;
    width: 100%;
    border: none !important;			
    &:hover {
	    background: #e8e8e8;
    }
	}
  &.inline {
    position: static;
    border: 0;  
    display: block!important;
    text-align: left;
    padding-bottom: 0;
    padding-top: 0;
    li {
      display: inline-block;
      min-width: 32px;
      width: auto;
      height: 32px;
      margin-right: 5px;
      margin-bottom: 5px;
      border: 1px solid #aaa!important;
      padding: 0 5px;
      line-height: 33px;
      &.active {
        background: $color-dark;
        color: white;
      }
      &.unavailable:hover {
        background: white;
      }

      @media screen and (max-width: 484px) {
        min-width: 40px;
        height: 40px;
        line-height: 40px;
      }
    }
  }
}

