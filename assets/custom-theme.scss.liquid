// new product page
.product-info {
  position: relative;
  .col-1 {
    float: left;
    width: 10%;
  }
  .col-2 {
    float: left;
    width: 53%;
    @include breakpoint(phone){
      float: none;
      width: 100%;
    }
    #product-main-photos {
      .gallery {
        float: none;
        width: 100%;
        margin-bottom: 10px;
      }
    }
    #product-photos {
      img {
        width: 100%;
        margin-left: 0;
        margin-right: 0;
      }
    }
    #content-product {
      min-height: 0;
      margin-bottom: 0;
    }
    .mobile.flexslider {
      display: none;
      margin-bottom: 0;
      border: 0;
      ul {
        li {
          padding-left: 0;
          margin-left: 0;
          line-height: 1;
        }
      }
    }
    @include breakpoint(phone){ 
      #product-photos {
        display: none;
      }
      .mobile.flexslider {
        display: block;
      }
    }
  }
  .col-3 {
    //position: absolute;
    width: 47%;
    padding: 3rem 6rem;
   // right: 0;
    float:right;
    &.fixed {
      top: 60px;
      position: fixed;
      overflow-y: scroll;
      z-index: 1;
    }
    .selector-wrapper {
      text-align: left;
    }
    #size-guide-trigger {
      width: auto;
      font-size: 1.25rem;
    }
    > h5 {
      padding-bottom: 1rem;
    }
    .product-description {
      p {
        font-size: 1.25rem;
        line-height: 2rem;
        margin-bottom: 5px;
      }
      h3 {
        font-size: 1.45rem;
        line-height: 2.4rem;
        padding-bottom: 6px;
      }
    }
    h5 > span {
      font-family: 'neuzeit-grotesk';
      font-weight: lighter;
      font-size: 1.5rem;
      letter-spacing: .04em;
      display: inline-block;
      margin-left: 20px;
    }
    @include breakpoint(phone){
      position: static!important;
      max-width: 100%!important;
      width: 100%;
      padding: 0;
      margin-top: 20px;
    }
  }
  .product-meta-wrapper {
    position: relative;
    margin-top: 10px;
    margin-bottom: 30px;
    clear: both;
    > div.show {
      h6 > span {
        display: inline-block;
        transform: rotate(90deg);
      }
      > div {
        display: block;
        opacity: 1;
      }
    }
    > div {
      h6 > span {
        display: inline-block;
      }
      > div {
        display: none;
      }
    }
    h6 {
      cursor: pointer;
      // float: left;
      // width: 33%;
      clear: both;
      font-size:1.5rem;
      padding-bottom: 5px;
      padding-top: 5px;
      margin-bottom: 0;
      @include breakpoint(phone){
        float: none;
        width: 100%;
        margin-top: 20px;
      }
      span {
        line-height: 1;
        font-weight: bold;
        font-size: 1;
      }
    }
    .product-fit {
      > div {
        opacity: 1;
        z-index: 1;
        // position: absolute;
        right: 0;
        // width: 66%;
        background: white;
        @include breakpoint(phone){
          position: static;
          width: 100%;
        } 
      }
      
      img {max-width: 75%;
        padding-bottom:10px;
      }
      p {
        font-size: 1.35rem;
        line-height: 1.4;
        margin-bottom: 5px;
        padding-left: 0;
      }
    }
    .product-fabric > div {
      z-index: 2;
      padding-bottom:10px;
    }
    .product-facts > div{
      z-index: 3;
      padding-bottom:5px;
    }
    .product-tip > div{
      z-index: 4;
      padding-bottom:5px;
    }
    .product-fabric,
    .product-facts,
    .product-tip,
     .product-fit, {
      > div {
        opacity: 0;
        // position: absolute;
        right: 0;
        // width: 66%;
        background: white;
        @include breakpoint(phone){
          position: static;
          width: 100%;
          display: block;
          opacity: 1;
        }
        p {
          font-size: 1.25rem;
          line-height: 1.35;
          margin-bottom: 10px;
          padding-left: 0;
        }
        li {
          font-size: 1.25rem;
          line-height: 1.35;
          margin-bottom: 5px;
          padding-left: 0;
        }
      }
    }
  }
  .product-form-wrapper {
    span#size-guide-trigger {
      // margin-right: 10px;
    }
    #add-button {
      float: none!important;
      width: 100%!important;
      max-width: 100%;
    }
    input#add {
      padding-top: 15px;
      padding-bottom: 15px;
      min-width: 250px;
      max-width: 100%;
      width: 100%;
      @include breakpoint(phone){
        margin-top: 22px;
        font-size: 1.5rem;
      }
    }
    span.preorder-msg {
      display: block;
      margin-top: 5px;
      text-align: center;
    }
    .size-title {
      display: inline-block;
      margin-bottom: 5px;
      font-size: 1.35rem;
    } 
    .swatch {
      margin-bottom: 20px;
      .swatch-container {
        position: static;
        transform: none;
      }
      .swatch-title {
        margin: 0;
        position: static;
        margin-bottom: 0;
        top: 0;
        transform: none;
        padding-bottom: 5px;
      }
      .sw-col {
        float: none;
        width: 100%!important;
      }
      .swatch-element {
        float: none;
        display: inline-block;
        margin-right: 12px;
        margin-bottom: 0;
        label {
          width: 30px;
          height: 30px;
          &::after {
            width: 36px;
            height: 36px;
            margin-top: -19px;
            margin-left: -19px;
          }

          @media screen and (max-width: 484px) {
            width: 40px;
            height: 40px;
          }
        }
      }
    }
    .colors {
      margin-bottom: 20px;
      .selected-color {
        margin-bottom: 0;
      }
      > .color {
        width: 30px;
        height: 30px;
        border-width: 1px;
        border-style: solid;
        border-color: #c2c2c2;
        border-radius: 10em;
        padding: 2px;
        background: white;
        display: inline-block;
        > div {
          height: 100%;
          width: 100%;
          border-radius: 10em;
        }
      }
    }
    .sizes {
      p {
        float: left;
        margin-bottom: 0;
      }
      p.small {
        float: right;
        margin-bottom: 0;
        font-size: 1.2rem;
        color: #aaa;
      }
      div {
        clear: both;
        div.size { 
          list-style: none;
          padding: 0;
          border: 1px solid #aaa;
          display: inline-block;
          margin-right: 5px;
          margin-left: 0;
          margin-bottom: 5px;
          width: 37px;
          height: 37px;
          text-align: center;
          line-height: 35px;
          &:nth-child(10) {
            margin-right: 0;
          }
          input {
            border: 0;
            padding: 0;
            margin: 0;
            &:checked + label {
              background-color: #001a33;
              border-color: #001a33;
              color: white;
            } 
          }
          label {
            font-size: 1.4rem;
            opacity: .5;
            display: block;
            transition: all .2s;
          }
          &.available {
            label {
              opacity: 1;
              cursor: pointer;
              &:hover {
                background-color: #001a33;
                border-color: #001a33;
                color: white;
              }
            }
          }
        }
      }
    }
  }
}


