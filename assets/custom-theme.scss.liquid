// ---------- Sass Mixins ----------------------- //
@mixin clearfix(){
  &:after {
    content: "";
    display: table;
    clear: both; }
  *zoom: 1;
}

@mixin prefix($property, $value){
  -webkit-#{$property}: #{$value};
  -moz-#{$property}: #{$value};
  -ms-#{$property}: #{$value};
  -o-#{$property}: #{$value};
  #{$property}: #{$value};
}

@mixin breakpoint($point){
  @if $point == xxl {
    @media (min-width: 1270px) { @content; }
  }
  @if $point == xl {
    @media (min-width: 1080px) { @content; }
  }
  @else if $point == l {
    @media (min-width: 1024px) { @content; }
  }
  @else if $point == ml {
    @media (min-width: 920px) { @content; }
  }  
  @else if $point == m {
    @media (min-width: 832px) { @content; }
  }
  @else if $point == s {
    @media (min-width: 680px) { @content; }
  }
  @else if $point == xs {
	  @media (min-width: 500px) { @content; }
  }
  @else if $point == ipad {
  	@media (min-width: 481px) and (max-width: 990px) {@content;}
  }
  @else if $point == phone {
  	@media (min-width: 320px) and (max-width: 480px) {@content;}
  }
}

@mixin centervert($height){
	height: $height;
	margin-top: - $height/2;
}

@mixin fillScreen($offset){
	height: calc(50vh - #{$offset});
}


@mixin row($width) {
  font-size: 0;
  letter-spacing: -.02rem;
  word-spacing: -.02rem;  
  max-width: $width;
  margin: 0 auto;
}

@mixin col($col, $sum, $gap: 1em, $align: top, $first: false, $last: false) {
  width: percentage($col/$sum);
  display: inline-block;
  vertical-align: $align;
  box-sizing: border-box;
  padding-left: if($first, 0, $gap);
  padding-right: if($last, 0, $gap);
}

// ---------- Sass Extends ---------------------- //
.transition {
	@include prefix('transition', all 0.3s ease-out);
}

.vert-y-wrap {
	position: relative;
	@include prefix('transform-style', preserve-3d);
}

.vert-y {
	position: absolute;
	top: 50%;
	@include prefix('transform', translateY(-50%));
}

.unvert-y {
	position: inherit;
	top: inherit;
	@include prefix('transform', translateY(0));		
	@include breakpoint(m){
		position: absolute;
		top: 50%;
		@include prefix('transform', translateY(-50%));	
	}
}

.vert-x {
	position: absolute;
	left: 50%;
	@include prefix('transform', translateX(-50%));
}

.unvert-x {
	position: inherit;
	left: inherit;
	@include prefix('transform', translateX(0));		
	@include breakpoint(m){
		position: absolute;
		left: 50%;
		@include prefix('transform', translateX(-50%));	
	}
}

.unvert-x-mobile {
	position: absolute;
	left: 50%;
	@include prefix('transform', translateX(-50%));	
	@include breakpoint(m){
		position: inherit;
		left: inherit;
		@include prefix('transform', translateX(0));		
	}
}

.last-cell {
	width: 16%;
	padding-left: 3%;
}

.label {
	text-transform: uppercase;
}

.selector-labels {
	text-transform: uppercase;
	font-size: 1.4rem;
}

.title {
	text-transform: uppercase;
	font-weight: 600;
	letter-spacing: $spacing-wider;
	color: $color-dark;
}

.pre-title {
	font-size: .8rem;
	margin-bottom: .5rem;
	display: inline-block;
	letter-spacing: $spacing-wide;
	text-transform: uppercase;
	color: $color-brand;
	@include breakpoint(m){
		font-size: 1.5rem;
		margin-bottom: 1rem;
	}	
}

.hover-header {
	&:hover {
		.anim-img {
			@include prefix('opacity', .8);
		}
	}
	.anim-img {
		display: block;
		@extend .transition;
	}	
}




